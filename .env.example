# Example environment variables for local development
# Copy this file to .env and fill in secrets as needed
# WARNING: Never commit .env file with real secrets to version control

# PostgreSQL connection string
DATABASE_URL=postgresql://postgres:your_secure_password@postgres:5432/permit_db?schema=public

# Redis configuration
REDIS_URL=redis://redis:6379
REDIS_HOST=redis
REDIS_PORT=6379

# JWT Secret (used for authentication tokens)
JWT_SECRET=your_long_random_secret_here_min_32_chars

# OpenTelemetry endpoint
OTEL_EXPORTER_OTLP_ENDPOINT=http://jaeger:4318/v1/traces

# Grafana admin password
GRAFANA_ADMIN_PASSWORD=admin

# PostgreSQL admin credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=permit_db
